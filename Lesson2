#include <stdio.h>
#include <math.h>
int main()
{
    unsigned int N;
    unsigned int i, d, l;
    scanf("%d", &N);
    for (d=2; d<=N; d++)
    {

        l=0;

        for (i=2; i<= sqrt(d); i++ )

            {
                

                if(d%i==0)

                l++; 
                continue;

                

          

            }   

        if (l==0)

        printf ("%d\n", d);

    

    }

    

}
